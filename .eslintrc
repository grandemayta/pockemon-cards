{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "rules": {
      "comma-dangle": 0,
      "arrow-parens": ["error", "as-needed"],
      "no-console": "off",
      "jsx-a11y/anchor-is-valid": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
      "max-len": ["error", 140],
      "react/forbid-prop-types": [false]
    }
}
